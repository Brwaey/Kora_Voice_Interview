{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
        "source": "/api/(.*)",
        "destination": "/api/app"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/vercel.py"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ]
}